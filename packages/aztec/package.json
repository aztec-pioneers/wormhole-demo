{
  "name": "@aztec-wormhole-demo/aztec-contracts",
  "version": "1.0.0",
  "description": "Aztec contracts and TypeScript library for Wormhole demo",
  "license": "MIT",
  "type": "module",
  "main": "./ts/index.ts",
  "types": "./ts/index.ts",
  "exports": {
    ".": {
      "import": "./ts/index.ts",
      "types": "./ts/index.ts"
    },
    "./artifacts": {
      "import": "./ts/artifacts/index.ts",
      "types": "./ts/artifacts/index.ts"
    },
    "./fees": {
      "import": "./ts/fees.ts",
      "types": "./ts/fees.ts"
    },
    "./constants": {
      "import": "./ts/constants.ts",
      "types": "./ts/constants.ts"
    },
    "./utils": {
      "import": "./ts/utils.ts",
      "types": "./ts/utils.ts"
    }
  },
  "dependencies": {
    "@aztec/accounts": "3.0.0-devnet.5",
    "@aztec/aztec.js": "3.0.0-devnet.5",
    "@aztec/constants": "3.0.0-devnet.5",
    "@aztec/entrypoints": "3.0.0-devnet.5",
    "@aztec/ethereum": "3.0.0-devnet.5",
    "@aztec/foundation": "3.0.0-devnet.5",
    "@aztec/noir-contracts.js": "3.0.0-devnet.5",
    "@aztec/pxe": "3.0.0-devnet.5",
    "@aztec/stdlib": "3.0.0-devnet.5",
    "@aztec/test-wallet": "3.0.0-devnet.5",
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "typescript": "^5.7.2",
    "viem": "^2.21.53"
  },
  "scripts": {
    "build": "tsx ./scripts/compile.ts",
    "setup:accounts": "tsx ./scripts/setupAccounts.ts",
    "setup:relayerAccount": "tsx ./scripts/relayerAccount.ts",
    "setup:deploy": "tsx ./scripts/deploy.ts",
    "setup:configure": "tsx ./scripts/configure.ts",
    "deploy:evm": "tsx ./scripts/deployEvm.ts",
    "send": "tsx ./scripts/sendValue.ts",
    "send:evm": "tsx ./scripts/sendValueEvm.ts",
    "read": "tsx ./scripts/readValues.ts",
    "check:emitters": "tsx ./scripts/check-emitters.ts"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "tsx": "^4.19.2"
  }
}
