# EVM RPC URL (e.g., Arbitrum Sepolia)
EVM_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Private key for deployment and transactions
PRIVATE_KEY=

# Wormhole Core contract address on target chain
WORMHOLE_ADDRESS=

# Aztec Bridge address (set after deploying Aztec contract)
AZTEC_BRIDGE_ADDRESS=

# Chain ID for the EVM chain (Wormhole chain ID)
# Arbitrum Sepolia = 10003
CHAIN_ID=10003

# Finality (number of blocks to wait)
FINALITY=1
